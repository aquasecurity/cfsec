{
  "configurations": {
    "Main": {
      "adapter": "vscode-go",
      "configuration": {
        "request": "launch",
        "program": "${workspaceFolder}/cmd/cfsec",
        "args": [
          "example/bucket.json"
        ],
        "dlvToolPath": "$HOME/go/bin/dlv"
      }
    },
  "Run Test": {
      "adapter": "vscode-go",
      "configuration": {
        "request": "launch",
        "program": "${fileDirname}",
        "mode": "test",
        "dlvToolPath": "$HOME/go/bin/dlv",
		 "args": [
                "-test.v",
                "-test.run",
                "${Test}"
            ]
  	  }
    }

}
}
